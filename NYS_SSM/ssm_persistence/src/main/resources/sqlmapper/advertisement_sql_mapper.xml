<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.niyongsheng.persistence.dao.AdvertisementDao">

    <select id="selectAdvertisementList" resultType="com.niyongsheng.persistence.domain.Advertisement">
        select *
        from dcd_advertisement
        where fellowship = #{fellowship} and type = #{type}
    </select>

</mapper>