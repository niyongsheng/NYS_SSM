<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.niyongsheng.persistence.dao.PlateMissionDao">

    <select id="selectUnfinishedMissionList" resultType="com.niyongsheng.persistence.domain.PlateMission">
        select *
        from dcd_plateMission
        where serialno = #{serialno} and status = 0
    </select>

</mapper>