<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.niyongsheng.persistence.dao.ConversationDao">

    <select id="getConversationListByFellowship" resultType="com.niyongsheng.persistence.domain.Conversation">
        select *
        from dcd_conversation c
        where c.fellowship = #{fellowship}
    </select>
</mapper>