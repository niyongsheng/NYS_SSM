<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.niyongsheng.persistence.dao.PrayDao">
    <select id="selectAllMultiTable" resultType="com.niyongsheng.persistence.domain.Pray">
        select *
        from dcd_pray a
        inner join dcd_fellowship b
        on a.fellowship = b.id
    </select>

    <select id="selectAllByFellowshipMultiTable" resultType="com.niyongsheng.persistence.domain.Pray">
        select *
        from dcd_pray a
        inner join dcd_fellowship b
        on a.fellowship = b.id
        where a.fellowship = #{fellowship}
    </select>
</mapper>