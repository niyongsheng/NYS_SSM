<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.niyongsheng.persistence.dao.User_ActivityDao">

    <select id="selectIsInGroupByUserAccount" resultType="java.lang.Boolean">
        select (*, false)
        from dcd_user_activity
        where account = #{account}
    </select>

</mapper>